<%= render 'layouts/message_display' %>
<table class="table table-striped table-bordered table-condensed">
  <th>EMAIL</th>
  <th>TOKEN</th>
  <th>SENT ON</th>
  <% @invitations.each do |invitation|%>
      <tr>
        <td><%= invitation.email %></td>
        <td><%= invitation.token %></td>
        <td><%= invitation.created_at %></td>
      </tr>
  <% end %>
</table>