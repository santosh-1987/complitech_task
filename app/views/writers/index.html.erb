<table class="table table-striped table-bordered table-condensed">
  <th>Email</th>
  <th colspan=2>Action</th>
  <% @writers.each do |writer| %>
      <tr>
        <td><%= writer.email%></td>
        <td style="text-align: center !important;" ><%= link_to 'Edit', edit_writer_path(writer) %></td>
        <td style="text-align: center !important;"><%= link_to 'Destroy',writer_path(writer) ,:method => :delete , :data=>{ :confirm=> 'Are you sure?' } %></td>
      </tr>
  <% end %>
 </table>
<%= link_to "Back",dashboard_home_index_path%>
